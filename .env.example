# Copy to .env and fill values. Never commit secrets.
PORT=3014

# Google OAuth (example)
GOOGLE_CLIENT_ID=
GOOGLE_CLIENT_SECRET=
GOOGLE_REDIRECT_URI=http://localhost:3014/api/gbp/oauth/callback

# Supabase
SUPABASE_URL=
SUPABASE_ANON_KEY=
SUPABASE_SERVICE_ROLE_KEY=

# App secret for signing cookies/state
APP_SECRET=change_me_32chars_minimum

# Security (production)
# Under HTTPS, set COOKIE_SECURE=1 to add Secure flag to cookies
COOKIE_SECURE=0
# Comma-separated list of allowed origins for CORS. Leave empty for dev (wide-open).
ALLOWED_ORIGINS=

# Notifications
# none | console | webhook
NOTIFY_PROVIDER=none
NOTIFY_WEBHOOK_URL=

# Outbox retry (optional)
# Max attempts before marking a task as failed (default 9)
OUTBOX_MAX_ATTEMPTS=9
